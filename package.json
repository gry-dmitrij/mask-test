{
  "name": "atom7-front",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nuxt",
    "analyze": "cross-env NODE_ENV=production nuxt build --analyze",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore --ignore-pattern dist",
    "prepare": "husky install",
    "lint": "yarn lint:js && yarn lint:style",
    "prettier": "prettier --write ."
  },
  "lint-staged": {
    "*.{js,json,css,scss,vue}": "pretty-quick --staged",
    "*.{js,vue}": "eslint",
    "*.{css,scss,vue}": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1602966760.822bc05",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dayjs": "^1.4.1",
    "@nuxtjs/sentry": "^5.1.5",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/svg": "^0.3.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.15.1",
    "cross-env": "^7.0.3",
    "imask": "^6.4.2",
    "lodash-es": "^4.17.21",
    "node-sass": "^6.0.1",
    "nuxt": "^2.15.7",
    "sass": "^1.42.1",
    "sass-loader": "^10",
    "smoothscroll-polyfill": "^0.4.4",
    "v-hotkey": "^0.9.0",
    "vee-validate": "^3.4.13",
    "vue-async-computed": "^3.9.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/composition-api": "^0.32.0",
    "@nuxtjs/eslint-config": "^7.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0"
  }
}
